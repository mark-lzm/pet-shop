<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.litchi.petshop.product.dao.ProductDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.litchi.petshop.product.entity.ProductEntity" id="productMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="catId" column="cat_id"/>
        <result property="unit" column="unit"/>
        <result property="specifications" column="specifications"/>
        <result property="lastPurchasePrice" column="last_purchase_price"/>
        <result property="costPrice" column="cost_price"/>
        <result property="salePrice" column="sale_price"/>
        <result property="stock" column="stock"/>
        <result property="remarks" column="remarks"/>
        <result property="productInfoMark" column="product_info_mark"/>
    </resultMap>


</mapper>